&cpu0 {
	cpu-supply = <&vddcpu>;
	operating-points-v2 = <&cpu_opp_table>;
	clocks = <&clkc CLKID_CPU_CLK>,
	       <&clkc CLKID_CPU_CLK_DYN>,
	       <&clkc CLKID_SYS_PLL>;
	clock-names = "core_clk",
		"low_freq_clk_parent",
		"high_freq_clk_parent";
	clock-latency = <50000>;
};

&cpu1 {
	cpu-supply = <&vddcpu>;
	operating-points-v2 = <&cpu_opp_table>;
	clocks = <&clkc CLKID_CPU_CLK>,
	       <&clkc CLKID_CPU_CLK_DYN>,
	       <&clkc CLKID_SYS_PLL>;
	clock-names = "core_clk",
		"low_freq_clk_parent",
		"high_freq_clk_parent";
	clock-latency = <50000>;
};

&cpu2 {
	cpu-supply = <&vddcpu>;
	operating-points-v2 = <&cpu_opp_table>;
	clocks = <&clkc CLKID_CPU_CLK>,
	       <&clkc CLKID_CPU_CLK_DYN>,
	       <&clkc CLKID_SYS_PLL>;
	clock-names = "core_clk",
		"low_freq_clk_parent",
		"high_freq_clk_parent";
	clock-latency = <50000>;
};

&cpu3 {
	cpu-supply = <&vddcpu>;
	operating-points-v2 = <&cpu_opp_table>;
	clocks = <&clkc CLKID_CPU_CLK>,
	       <&clkc CLKID_CPU_CLK_DYN>,
	       <&clkc CLKID_SYS_PLL>;
	clock-names = "core_clk",
		"low_freq_clk_parent",
		"high_freq_clk_parent";
	clock-latency = <50000>;
};

